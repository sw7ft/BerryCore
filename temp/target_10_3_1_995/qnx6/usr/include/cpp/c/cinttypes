// cinttypes standard header
#ifndef _CINTTYPES_
#define _CINTTYPES_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <inttypes.h>
 #define _STD_USING

#else /* _STD_USING */
 #include <inttypes.h>

 #ifdef _GLOBAL_USING
_STD_BEGIN
using _CSTD imaxdiv_t;

using _CSTD abs; using _CSTD div;
using _CSTD imaxabs; using _CSTD imaxdiv;
using _CSTD strtoimax; using _CSTD strtoumax;
using _CSTD wcstoimax; using _CSTD wcstoumax;
_STD_END
 #endif /* _GLOBAL_USING */

#endif /* _STD_USING */
#endif /* _CINTTYPES_ */

